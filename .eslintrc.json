{
  "env": {
      "browser": true,
      "es2021": true
  },
  "parser": "@typescript-eslint/parser",
  "overrides": [{
    "files": ["*.ts"],
    "extends": [
      "airbnb-base",
      "airbnb-typescript/base"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "rules": {
      "linebreak-style": "off",
      "import/prefer-default-export": "off"
    }
  }]
}